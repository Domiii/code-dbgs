	"scripts": {
		"start": "npx npm-run-all -p build:watch graph",
		"build:watch": "npx nodemon --exec \"npx webpack --watch --mode development\"",
		"graph": "cd dbux-graph-client && npm start",
		"dbux-install": "yarn install && sh scripts/dbux-run-all.sh \"yarn install\"",
		"dbux-install-force": "yarn install --force",
		"dbux-all": "sh scripts/dbux-run-all.sh",
		"dbux-uninstall": "rm -rf ./node_modules && rm -f ./package-lock.json && sh scripts/dbux-run-all.sh \"rm -rf ./node_modules && rm -f ./package-lock.json\"",
		"p1-install": "sh ./assets/projects/todomvc-vanilla-es6/install.sh",
		"p1-go": "cd ./projects/todomvc/examples/vanilla-es6/ && code ./webpack.config.js",
		"p1-start": "cd ./projects/todomvc/examples/vanilla-es6/ && npm start",
		"dbux-code-start": "cd ./dbux-code && npm start",
		"cloc": "npx cloc --match-f='.*\\.((js)|(py))$' --fullpath --match-d='(dbux-[^/]+|analysis|samples/__samplesInput__)' --not-match-d='(node_modules|dist)$'  --by-file .",
		"clinic": "clinic flame -- node dbux-cli/bin/run-js-unbabel ../samples/__samplesOutput__/oop1.es5.js || (echo \"\\033[1;31mCould not execute clinic - checking installed version:\\033[0m\" && clinic -v)"
	},
	"dependencies": {
		"@babel/runtime": "^7.8.3",
		"core-js": "^3.6.4",
		"find-package-json": "^1.2.0",
		"link-module-alias": "^1.2.0",
		"lodash": "^4.17.19",
		"nanoevents": "^2.0.0",
		"webpack-node-externals": "^1.7.2",
		"colors": "*",
    "yargs": "^15.1.0",
    "shelljs": "*"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.1",
		"utf-8-validate": "^5.0.2"
	}
}
